{
  "editor.tabSize": 2,
  "editor.fontSize": 18,
  "editor.lineHeight": 26,
  // "editor.fontFamily": "Fira Code Retina",
  "editor.fontFamily": "JetBrains Mono",
  "editor.letterSpacing": 0.5,
  "editor.minimap.enabled": true,
  "editor.formatOnPaste": true,
  "editor.formatOnType": false,
  "editor.tabCompletion": "on",
  "workbench.editor.limit.enabled": true,
  "workbench.editor.limit.value": 5,
  "liveshare.increasedGuestLimit": false,
  "liveshare.codeLens": false,
  "git.confirmSync": false,
  "files.hotExit": "onExit",
  "files.defaultLanguage": "",
  "files.trimTrailingWhitespace": true,
  "editor.cursorSmoothCaretAnimation": true,
  "liveServer.settings.donotShowInfoMsg": true,
  "editor.cursorBlinking": "expand",
  "editor.cursorStyle": "line",
  "editor.autoIndent": "full",
  "editor.fontLigatures": true,
  "explorer.compactFolders": false,
  "editor.wordWrapColumn": 96,
  "editor.wordWrap": "wordWrapColumn",
  "editor.rulers": [
    96
  ],
  "editor.formatOnSave": true,
  "[java]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[javareact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[type]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "[typereact]": {
    "editor.codeActionsOnSave": {
      "source.fixAll.eslint": true
    }
  },
  "files.associations": {
    ".stylelintrc": "json",
    ".prettierrc": "json"
  },
  "editor.renderLineHighlight": "gutter",
  "terminal.integrated.fontSize": 18,
  "workbench.startupEditor": "newUntitledFile",
  "window.zoomLevel": 0.5,
  "extensions.ignoreRecommendations": true,
  "emmet.syntaxProfiles": {
    "java": "jsx"
  },
  "breadcrumbs.enabled": false,
  "git.enableSmartCommit": true,
  "peacock.keepBadgeColor": true,
  "peacock.elementAdjustments": {
    "activityBar": "none",
    "statusBar": "none",
    "titleBar": "none"
  },
  "editor.parameterHints.enabled": false,
  "terminal.integrated.shell.linux": "/bin/zsh",
  "explorer.confirmDragAndDrop": false,
  "explorer.confirmDelete": false,
  "workbench.activityBar.visible": true,
  "peacock.favoriteColors": [
    {
      "name": "Artur",
      "value": "#42072c"
    },
    {
      "name": "Artur 2",
      "value": "#2b051d"
    },
    {
      "name": "DARK",
      "value": "#010003"
    }
  ],
  "window.menuBarVisibility": "toggle",
  "window.titleBarStyle": "native",
  "files.autoSave": "off",
  "workbench.colorCustomizations": {
    "[Night Wolf [dark gray]]": {
      "activityBar.activeBorder": "#f9f9f9",
    },
    "[Night Wolf [gray]]": {
      "activityBar.activeBorder": "#f9f9f9",
    },
    "[Moonlight II Italic]": {
      "activityBar.activeBorder": "#f9f9f9",
      "activityBarBadge.background": "#f9f9f9",
      "activityBarBadge.foreground": "#030307",
      "editorLineNumber.activeForeground": "#d9d9d9",
      "activityBar.inactiveForeground": "#bebebe",
      "sideBar.background": "#0b011c",
      "sideBarSectionHeader.background": "#0b011c",
      "terminal.background": "#0b011c",
      "panel.background": "#0b011c",
      "tab.activeBackground": "#182030",
      "tab.inactiveBackground": "#0b011c",
      // "editor.background": "#11121f",
      // "editor.background": "#100424",
      "editor.background": "#100126",
      "editorLineNumber.foreground": "#9d9d9f",
      "editorGroupHeader.tabsBackground": "#0b011c"
    },
  },
  "typescript.updateImportsOnFileMove.enabled": "always",
  "javascript.updateImportsOnFileMove.enabled": "always",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.colorTheme": "Moonlight II Italic"
}
